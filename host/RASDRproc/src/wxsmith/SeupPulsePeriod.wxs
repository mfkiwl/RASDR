<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxDialog" name="SeupPulsePeriod">
		<pos_arg>1</pos_arg>
		<size_arg>1</size_arg>
		<object class="wxFlexGridSizer" variable="FlexGridSizer1" member="no">
			<cols>3</cols>
			<object class="sizeritem">
				<object class="wxRadioBox" name="ID_RADIOBOX1" variable="FileType" member="yes">
					<label>File Type</label>
					<content>
						<item>Excel (Limitid Size) (.csv)</item>
						<item>Geenral (.csv)</item>
					</content>
					<default>-1</default>
					<font>
						<size>12</size>
						<style>normal</style>
						<weight>bold</weight>
						<underlined>0</underlined>
						<family>swiss</family>
						<face>Times New Roman</face>
					</font>
					<handler function="OnFileTypeSelect" entry="EVT_RADIOBOX" />
				</object>
				<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
				<border>5</border>
				<option>1</option>
			</object>
			<object class="sizeritem">
				<object class="wxRadioBox" name="ID_RADIOBOX2" variable="NumberofFiles" member="yes">
					<label>Number of Files</label>
					<content>
						<item>Single (Append)</item>
						<item>Multiple (Suffix Name)</item>
					</content>
					<default>-1</default>
					<font>
						<size>12</size>
						<style>normal</style>
						<weight>bold</weight>
						<underlined>0</underlined>
						<family>swiss</family>
						<face>Times New Roman</face>
					</font>
					<handler function="OnNumberofFilesSelect" entry="EVT_RADIOBOX" />
				</object>
				<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
				<border>5</border>
				<option>1</option>
			</object>
			<object class="sizeritem">
				<object class="wxRadioBox" name="ID_RADIOBOX3" variable="TimeStandard" member="yes">
					<label>Time Standard</label>
					<content>
						<item>Local Time (i.e. EST)</item>
						<item>Universal Time (UT)</item>
					</content>
					<default>-1</default>
					<font>
						<size>12</size>
						<style>normal</style>
						<weight>bold</weight>
						<underlined>0</underlined>
						<family>swiss</family>
						<face>Times New Roman</face>
					</font>
					<handler function="OnTimeStandardSelect" entry="EVT_RADIOBOX" />
				</object>
				<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
				<border>5</border>
				<option>1</option>
			</object>
			<object class="sizeritem">
				<object class="wxStaticText" name="ID_STATICTEXT1" variable="StaticText1" member="yes">
					<label>Enter File Name : </label>
					<font>
						<size>12</size>
						<style>normal</style>
						<weight>bold</weight>
						<underlined>0</underlined>
						<family>swiss</family>
						<face>Times New Roman</face>
					</font>
				</object>
				<flag>wxALL|wxALIGN_RIGHT|wxALIGN_CENTER_VERTICAL</flag>
				<border>5</border>
				<option>1</option>
			</object>
			<object class="sizeritem">
				<object class="wxFilePickerCtrl" name="ID_FILEPICKERCTRL1" variable="PulseFile" member="yes">
					<message></message>
					<path>/PulseOut.csv</path>
					<wildcard>*.csv&quot;</wildcard>
					<font>
						<size>12</size>
						<style>normal</style>
						<weight>bold</weight>
						<underlined>0</underlined>
						<family>swiss</family>
						<face>Times New Roman</face>
					</font>
					<handler function="OnPulsePeriodFileFileChanged" entry="EVT_FILEPICKER_CHANGED" />
				</object>
				<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
				<border>5</border>
				<option>1</option>
			</object>
			<object class="sizeritem">
				<object class="wxCheckBox" name="ID_CHECKBOX1" variable="Overwrite" member="yes">
					<label>OVERWRITE?</label>
					<font>
						<size>12</size>
						<style>normal</style>
						<weight>bold</weight>
						<underlined>0</underlined>
						<family>swiss</family>
						<face>Times New Roman</face>
					</font>
					<handler function="OnOverwriteClick" entry="EVT_CHECKBOX" />
				</object>
				<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
				<border>5</border>
				<option>1</option>
			</object>
			<object class="sizeritem">
				<object class="wxFlexGridSizer" variable="FlexGridSizer2" member="no">
					<cols>3</cols>
					<object class="sizeritem">
						<object class="wxButton" name="ID_BUTTON1" variable="OK" member="yes">
							<label>OK</label>
							<font>
								<size>12</size>
								<style>normal</style>
								<weight>bold</weight>
								<underlined>0</underlined>
								<family>swiss</family>
								<face>Times New Roman</face>
							</font>
							<handler function="OnOKClick" entry="EVT_BUTTON" />
						</object>
						<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxButton" name="ID_BUTTON2" variable="Cancel" member="yes">
							<label>CANCEL</label>
							<font>
								<size>12</size>
								<style>normal</style>
								<weight>bold</weight>
								<underlined>0</underlined>
								<family>swiss</family>
								<face>Times New Roman</face>
							</font>
							<handler function="OnCancelClick" entry="EVT_BUTTON" />
						</object>
						<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
						<border>5</border>
						<option>1</option>
					</object>
				</object>
				<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
				<border>5</border>
				<option>1</option>
			</object>
		</object>
	</object>
</wxsmith>
